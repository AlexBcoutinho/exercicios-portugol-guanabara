programa
{
	
	funcao inicio()
	{
		inteiro n1
	
		escreva("Digite um numero: ")
			leia (n1)

		se ( n1%3==0)

			escreva("O valor digitado é múltiplo de 3")
		senao
			escreva("O valor digitado não é múltiplo de 3")
	}
}
